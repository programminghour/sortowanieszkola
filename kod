#include <iostream>

using namespace std;

const int n=10;
int tablica[n]={2,3,1,7,9,6,4,8,5,0};

int sca2[10] = {0,2,8,9,1,3,7,10,12,13};

const int m = 3;

int tab1[n] = {1,2,3,4,5,6,7};
int tab2[m] = {6,7,8};
int tab[n+m];

void wprow(int n, int t[])
{
    for(int i = 0; i<n; i++)
        cin >> t[i];
}

void wypisz(int n, int t[])
{
    for(int i = 0; i<n; i++)
        cout << " "<<t[i];
}



void bubblesort(int n,int tablica[])
{
    int przechow;
    for(int i = 0; i<n-1; i++)
    {
        for(int j = 0; j<n-1-i; j++)
        {
            if(tablica[j]>tablica[j+1])
                {
                    przechow=tablica[j];
                    tablica[j]=tablica[j+1];
                    tablica[j+1]=przechow;
                }
        }
    }
}

void wybor(int n, int tab[])
{
    int przechow, index=0;

    for(int i = 0; i<n-1  ;i++)
    {
        index=i;
        for(int j = i+1; j<n ; j++)
            if(tab[index]>tab[j])
            {
            index=j;
            }
        przechow=tab[i];
        tab[i]=tab[index];
        tab[index]=przechow;
    }
}


void wstawianie1(int n, int tab[])
{
    for(int i = 0; i < n; i++)
    {
        int x = tab[i], j = i - 1;

        for(j=i-1; j>=0 && tab[j] > x;j--)
           {tab[j+1]=tab[j];


           }
            cout<<j<<endl;
        tab[j+1]=x;

    }
}

void scalanie(int tab[], int tab1[], int tab2[]) //tab - posortowane; tab1- jedna; tab2- druga
{
    int i1 = 0, i2 = 0;

    for(int j = 0; j < n+m; j++)
    {
        if(i1<n && i2<m)
        {
            if(tab1[i1]<tab2[i2])
            {
                tab[j]=tab1[i1];
                i1++;
            }
            else
            {
                tab[j]=tab2[i2]; i2++;
            }

        }
        else if(i1>=n)
        {
            tab[j]=tab2[i2];
            i2++;
        }
        else
        {
            tab[j]=tab1[i1];
            i1++;
        }

    }

}

void scal2(int tab[10],int s)
{
    int pom[10];
    for(int i = 0; i < 10  ;i++)
        pom[i]=tab[i];
    int i1 = 0, i2 = s;
    for(int j = 0; j<n; j++)
        if(i1<s && i2<10)
        {
            if(pom[i1]>pom[i2])
            {
                tab[j]=pom[i2];
                i2++;
            }
            else
            {
                tab[j]=pom[i1]; i1++;
            }

        }
        else if(i1>=s-1)
        {
            tab[j]=pom[i2];
            i2++;
        }
        else
        {
            tab[j]=pom[i1];
            i1++;
        }

}

int main()
{
    wprow(n,tablica);
    wypisz(n,tablica);
    wybor(n, tablica);
    cout << "\n Pososrtowany \n" ;
    wypisz(n,tablica);
    return 0;
}
